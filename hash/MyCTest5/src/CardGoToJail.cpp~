#include "CardGoToJail.h"

//-------------------------------------------------------------------------
CardGoToJail::CardGoToJail()
{
}

//-------------------------------------------------------------------------
void CardGoToJail::action(
        PlayerManager &i_players
        )
{
	std::cout<< i_players.getName() << " IS GOING TO JAIL!!" << std::endl;
    i_players.setJailed(true);
}

//-------------------------------------------------------------------------
CardGoToJail::~CardGoToJail()
{
}
