#include "CardGetOutOfJail.h"

//-------------------------------------------------------------------------
CardGetOutOfJail::CardGetOutOfJail()
{
}

//-------------------------------------------------------------------------
void CardGetOutOfJail::action(PlayerManager &i_players)
{
	std::cout<< i_players.getName() << " Gets a Get Out of Jail Card " << std::endl;
    i_players.setJailed(false);
}

//-------------------------------------------------------------------------
CardGetOutOfJail::~CardGetOutOfJail()
{
}
